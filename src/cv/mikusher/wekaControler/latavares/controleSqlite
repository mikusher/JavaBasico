-- Table: public.vendas

-- DROP TABLE public.vendas;

CREATE TABLE public.vendas
(
    sexo character varying(1) COLLATE pg_catalog."default",
    idade integer,
    filhos character varying COLLATE pg_catalog."default",
    gasta_muito character varying COLLATE pg_catalog."default",
    id bigint NOT NULL DEFAULT nextval('vendas_id_seq'::regclass),
    CONSTRAINT vendas_pkey PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.vendas
    OWNER to xopvyzuz;
COMMENT ON TABLE public.vendas
    IS 'vendas table';
    


-- Table: public.products

-- DROP TABLE public.products;

CREATE TABLE public.products
(
    name text COLLATE pg_catalog."default" NOT NULL,
    price numeric NOT NULL,
    id bigint NOT NULL DEFAULT nextval('products_id_seq'::regclass),
    CONSTRAINT products_pkey PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.products
    OWNER to xopvyzuz;
COMMENT ON TABLE public.products
    IS 'products table';